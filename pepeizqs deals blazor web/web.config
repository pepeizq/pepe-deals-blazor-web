<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<location path="." inheritInChildApplications="false">
		<system.webServer>
			<security>
				<requestFiltering>
					<denyUrlSequences>
						<add sequence="wp-login.php" />
						<add sequence="wp-admin" />
						<add sequence="xmlrpc.php" />
					</denyUrlSequences>
				</requestFiltering>
			</security>

			<handlers>
				<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
			</handlers>
			
			<aspNetCore processPath=".\pepeizqs_deals_blazor_web.exe" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="OutOfProcess" />
		</system.webServer>
	</location>
</configuration>