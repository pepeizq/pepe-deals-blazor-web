				estadoBundles = null;
				estado = false;
			}

			usuario.NotificationBundles = estado;

			await BaseDatos.Usuarios.Actualizar.Opcion("NotificationBundles", estado, usuario.Id);
		}
	}

	private async void ClickearCambioGratis()
